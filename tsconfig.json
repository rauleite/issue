    "outDir": "./dist/",
    "noImplicitAny": false,
    "allowSyntheticDefaultImports": true,
    "module": "es6",
    "target": "es5",
    "allowJs": true,
    "moduleResolution": "node",
    "sourceMap": true,
    "lib": [
      "webworker"
    ]
